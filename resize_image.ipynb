    "from PIL import Image\n",
    "\n",
    "# Open an image file\n",
    "img = Image.open(\"assets/img/Sangwoo_profile.webp\")\n",
    "\n",
    "# Resize the image\n",
    "new_width = 280\n",
    "new_height = 350\n",
    "resized_img = img.resize((new_width, new_height))\n",
    "\n",
    "# Save as WebP\n",
    "resized_img.save(\"resized_image.webp\", format=\"WEBP\", quality=80)"
   ]
  }
